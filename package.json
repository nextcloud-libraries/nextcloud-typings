{
  "name": "@nextcloud/typings",
  "version": "1.10.0",
  "description": "Nextcloud TypeScript typings",
  "keywords": [
    "nextcloud",
    "typescript",
    "types"
  ],
  "homepage": "https://github.com/nextcloud-libraries/nextcloud-typings#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/nextcloud-libraries/nextcloud-typings"
  },
  "license": "GPL-3.0-or-later",
  "author": "Nextcloud GmbH and Nextcloud contributors",
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts"
    },
    "./ocs": {
      "types": "./lib/ocs.d.ts"
    }
  },
  "main": "dist/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "@types/jquery": "3.5.16"
  },
  "devDependencies": {
    "@nextcloud/eslint-config": "^9.0.0-rc.5",
    "@types/node": "^24.0.1",
    "eslint": "^9.39.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": "^20.0.0 || ^22.0.0 || ^24.0.0"
  },
  "devEngines": {
    "packageManager": [
      {
        "name": "npm",
        "onFail": "error",
        "version": "^11.3.0"
      }
    ],
    "runtime": {
      "name": "node",
      "onFail": "error",
      "version": "^24.0.0"
    }
  }
}
